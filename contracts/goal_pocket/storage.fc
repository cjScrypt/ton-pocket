(int, int, slice, int, int) load_storage () inline {
    slice ds = get_data().begin_parse();
    return (ds~load_uint(64), ds~load_coins(), ds~load_coins(), ds~load_msg_addr(), ds~load_msg_addr(), ds~load_uint(32), ds~load_uint(32));
}

() save_storage (int contributed_amount, int target, slice owner_address, slice pocket_wallet_address, int start_time, int duration) impure inline {
    set_data(begin_cell()
        .store_uint(goal_id, 64)
        .store_coins(contributed_amount)
        .store_coins(target)
        .store_slice(owner_address)
        .store_slice(pocket_wallet_address)
        .store_uint(start_time, 32)
        .store_uint(duration, 32)
      .end_cell()
    );
}