(int, slice, slice, cell, int) load_storage() inline {
    slice ds = get_data().begin_parse();
    return (ds~load_coins(), ds~load_msg_addr(), ds~load_msg_addr(), ds~load_ref(), ds~load_coins());
}

() save_storage(int spendable_balance, slice owner_address, slice jetton_master_address, cell jetton_wallet_code, int locked_balance) impure inline {
    set_data(pack_pocket_wallet_data(spendable_balance, owner_address, jetton_master_address, jetton_wallet_code, locked_balance));
}